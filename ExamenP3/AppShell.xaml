
<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ExamenP3.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ExamenP3"
    Shell.FlyoutBehavior="Disabled"
    Title="ExamenP3 ">

    <TabBar>
        
        <ShellContent
            Title="Buscar Películas"
            ContentTemplate="{DataTemplate local:MovieSearchPage}"
            Route="MovieSearchPage" />

        
        <ShellContent
            Title="Películas Consultadas"
            ContentTemplate="{DataTemplate local:MoviesListPage}"
            Route="MoviesListPage" />
        
    </TabBar>
</Shell>
